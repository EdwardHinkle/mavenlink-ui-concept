{
  "env": {
    "jasmine": true
  },
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "comma-dangle": ["error", {"arrays": "always-multiline", "objects": "always-multiline"}],
    "func-names": "off",
    "import/prefer-default-export": "off",
    "object-curly-spacing": ["error", "never"],
    "quotes": ["error", "double", {"avoidEscape": true}],
    "react/jsx-filename-extension": "off",
    "semi": ["error", "never"]
  }
}
